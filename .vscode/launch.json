    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug via OpenOCD",
            "type": "gdb",
            "request": "attach",
            "remote": true,
            "target": ":3333",
            "executable": "build/bluepill_stm32cube_template.elf",
            "cwd": "${workspaceFolder}",
            "autorun": [
                "monitor init",
                "monitor reset halt",
                "load",
            ],
            "gdbpath": "arm-none-eabi-gdb"
        }
    ]
}